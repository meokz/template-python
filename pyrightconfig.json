{
    "include": [
        "src"
    ],
    "exclude": [
        "**/__pycache__",
        "**/.pytest_cache",
        "**/.ipynb_checkpoints"
    ],
    "ignore": [
        "**/libs"
    ],
    "reportTypeshedErrors": false,
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.7",
    "pythonPlatform": "Linux",
    "executionEnvironments": [
        {
            "root": "src"
        }
    ]
}
